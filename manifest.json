{
  "manifest_version": 2,
  "name": "random",
  "version": "0.0.0",
  "description": "Manifest json example",
  "page_action": {
    "default_icon": "icon16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "web_accessible.js"
  ],
  "permissions": [
    "https://*/*",
    "http://*/*"
  ]
}
